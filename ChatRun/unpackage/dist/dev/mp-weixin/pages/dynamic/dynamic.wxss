/* 公共样式 */
.contents {
  padding-top: 25px;
}
/* 顶部 */
.top-bar {
  z-index: 100;
  width: 92%;
  height: 88rpx;
  position: fixed;
  top: 0;
  left: 0;
  background: rgba(255, 255, 255, 0.96);
  border-bottom: 1rpx solid rgba(39, 20, 50, 0.1);
  padding-left: 32rpx;
  padding-right: 32rpx;
  padding-top: 25px;
  box-shadow: 1rpx 1rpx 20rpx 2rpx rgba(117, 112, 117, 0.2);
}
.top-bar .top-bar-left {
  float: left;
}
.top-bar .top-bar-left image {
  width: 68rpx;
  height: 68rpx;
  margin-top: 10rpx;
  border-radius: 16rpx;
}
.top-bar .top-bar-center {
  float: left;
}
.top-bar .top-bar-center .logo {
  display: inline-block;
  padding-top: 18rpx;
  font-size: 24rpx;
  color: #ffe431;
}
.top-bar .top-bar-center .title {
  line-height: 88rpx;
}
.top-bar .top-bar-right {
  float: right;
  box-shadow: 8rpx 10rpx 25rpx -5rpx rgba(117, 112, 117, 0.3);
  border-radius: 40rpx;
}
.top-bar .top-bar-right .search {
  display: inline-block;
  width: 70rpx;
  height: 78rpx;
  padding-left: 12rpx;
}
.top-bar .top-bar-right .add {
  width: 78rpx;
  height: 78rpx;
  display: inline-block;
}
.top-bar .top-bar-right ._i {
  width: 52rpx;
  height: 52rpx;
  display: inline-block;
  font-size: 19rpx;
  padding: 24rpx 5rpx 5rpx 15rpx;
}
/* 底部 */
.bottom {
  position: fixed;
  bottom: 20rpx;
  width: 100%;
  height: 104rpx;
  box-sizing: border-box;
  padding: 12rpx 32rpx;
  padding-bottom: env(safe-area-inset-bottom);
}
.bottom .bottom-btn {
  height: 80rpx;
  margin: 0 auto;
  background: #ffe431;
  border-radius: 20rpx;
  text-align: center;
  line-height: 80rpx;
  font-size: 32rpx;
  color: #272832;
}
.btn1 {
  height: 80rpx;
  border-radius: 20rpx;
  text-align: center;
  line-height: 80rpx;
  font-size: 32rpx;
  color: #272832;
}
.top-bar {
  background: #ffe431;
}
.top-bar .top-bar-right {
  margin-top: 4rpx;
}
.top-bar .top-bar-right .add {
  padding-left: 20rpx;
}
.main {
  padding: 88rpx 5rpx;
}
.main .result {
  padding-top: 32rpx;
}
.main .result .title {
  font-size: 44rpx;
  font-weight: 600;
  color: #272832;
  line-height: 60rpx;
}
.main .result .list {
  width: 100%;
  padding: 10rpx 0;
  display: flex;
}
.main .result .list image {
  float: left;
  width: 80rpx;
  height: 80rpx;
  border-radius: 20rpx;
}
.main .result .card {
  padding: 20rpx;
  background: #fafafa;
  width: 100%;
}
.main .result .card .names {
  margin-left: 100rpx;
}
.main .result .card .names .name {
  font-size: 36rpx;
  color: #272832;
  line-height: 50rpx;
}
.main .result .card .names .name .nameright {
  position: relative;
  bottom: 60rpx;
}
.main .result .card .names .name .nameright ._i {
  float: right;
  font-size: 35rpx;
}
.main .result .card .names .email {
  font-size: 24rpx;
  color: #272832;
  line-height: 28rpx;
}
.main .result .card .msg {
  font-size: 30rpx;
  float: left;
  width: 100%;
  padding: 20rpx 0;
}
.main .result .card .imgArea {
  display: flex;
  flex-wrap: wrap;
  padding-right: 30rpx;
  width: 100%;
}
.main .result .card .imgArea image {
  width: 250rpx;
  height: 250rpx;
  margin: 0 10rpx 10rpx 0;
}
.main .result .card .talk {
  font-size: 24rpx;
  color: #fa560a;
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}
.main .result .card .talk ._i {
  font-size: 30rpx;
}
.modify {
  position: fixed;
  z-index: 1;
  top: 90rpx;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: #fff;
}
.modify .modify-header {
  width: 100%;
  height: 88rpx;
  padding-top: 25px;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1rpx solid rgba(39, 20, 50, 0.1);
  box-shadow: 1rpx 1rpx 20rpx 2rpx rgba(117, 112, 117, 0.2);
}
.modify .modify-header .close {
  padding-left: 32rpx;
  font-size: 32rpx;
  color: #272832;
  line-height: 44rpx;
}
.modify .modify-header .define {
  padding-right: 32rpx;
  font-size: 35rpx;
  color: #5799ff;
  line-height: 88rpx;
}
.modify .modify-main {
  padding: 60rpx 60rpx;
  width: 100%;
}
.modify .modify-main .modify-text {
  min-height: 100rpx;
  overflow-y: visible;
  font-size: 13pt;
  line-height: 20pt;
  color: #353535;
}
.modify .modify-main .modify-img {
  display: flex;
  flex-wrap: wrap;
  justify-content: flex-start;
  align-content: space-between;
}
.modify .modify-main .modify-img image {
  width: 160rpx;
  height: 160rpx;
  margin: 0 10rpx 10rpx 0;
}
.modify .modify-main .modify-img ._i {
  font-size: 160rpx;
  color: #ededed;
}

